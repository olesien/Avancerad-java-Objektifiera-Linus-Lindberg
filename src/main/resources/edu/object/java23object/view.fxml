<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/20" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.object.java23object.Controller">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <BorderPane prefHeight="26.0" prefWidth="225.0">
      <left>
        <Button fx:id="setFileBtn" onAction="#onSetTile" prefHeight="25.0" prefWidth="115.0" style="-fx-background-color: lightblue;" text="Set JSON/CSV file!" textFill="#2c2c2c" BorderPane.alignment="CENTER" />
      </left>
      <right>
      
        <Button fx:id="addDataBtn" disable="true" onAction="#onAddDataClick" prefHeight="25.0" prefWidth="105.0" style="-fx-background-color: lightblue;" text="Add data!" BorderPane.alignment="CENTER" />
      </right>
   </BorderPane>
   <TableView fx:id="tableView" prefHeight="400.0" prefWidth="225.0">
     <columns>
     </columns>
   </TableView>
</VBox>
